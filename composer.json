{
    "name": "bagisto/bagisto",
    "description": "Bagisto Laravel ECommerce",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": {
        "clearent": {
            "type": "path",
            "url": "custom_packages/devvly/clearent",
            "options": {
                "symlink": true
            }
        },
        "laravel-pagebuilder": {
            "type": "path",
            "url": "custom_packages/hansschouten/laravel-pagebuilder",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-ftp": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-tokenizer": "*",
        "astrotomic/laravel-translatable": "^11.0.0",
        "authorizenet/authorizenet": "2.0.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "braintree/braintree_php": "5.0.0",
        "cweagans/composer-patches": "~1.0",
        "doctrine/dbal": "3.2.0",
        "elasticsearch/elasticsearch": "~7.0",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "flynsarmy/db-blade-compiler": "^5.5",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hansschouten/laravel-pagebuilder": "@dev",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "kalnoy/nestedset": "6.0.0",
        "konekt/concord": "^1.2",
        "laravel/framework": "^8.65",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.1",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "3.1.33",
        "mailersend/laravel-driver": "^1.0",
        "nwidart/laravel-modules": "^3.2",
        "paquettg/php-html-parser": "^3.0",
        "platformsh/laravel-bridge": "^2.1",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "scssphp/scssphp": "^1.10",
        "stil/xpath-selector": "^2.0",
        "stripe/stripe-php": "7.0",
        "symfony/css-selector": "^5.1",
        "tinify/tinify": "^1.5",
        "tymon/jwt-auth": "^1.0.0",
        "unisharp/laravel-filemanager": "^2.1",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.16.0",
        "laravel/sail": "^1.0.1",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.7",
        "codeception/codeception": "4.1.22",
        "codeception/module-asserts": "^1.1",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-laravel5": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery":"^1.4.4",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit":   "^9.5.10"
    },
    "replace": {
        "bagisto/laravel-user": "v0.1.0",
        "bagisto/laravel-admin": "v0.1.0",
        "bagisto/laravel-ui": "v0.1.0",
        "bagisto/laravel-core": "v0.1.0",
        "bagisto/laravel-attribute": "v0.1.0",
        "bagisto/laravel-checkout": "v0.1.0",
        "bagisto/laravel-customer": "v0.1.0",
        "bagisto/laravel-inventory": "v0.1.0",
        "bagisto/laravel-category": "v0.1.0",
        "bagisto/laravel-product": "v0.1.0",
        "bagisto/laravel-shop": "v0.1.0",
        "bagisto/laravel-theme": "v0.1.0",
        "bagisto/laravel-shipping": "v0.1.0",
        "bagisto/laravel-payment": "v0.1.0",
        "bagisto/laravel-sales": "v0.1.0",
        "bagisto/laravel-tax": "v0.1.0",
        "bagisto/laravel-api": "v0.1.0",
        "bagisto/laravel-paypal": "v0.1.0",
        "bagisto/laravel-discount": "v0.1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Webkul\\User\\": "packages/Webkul/User/src",
            "Webkul\\Admin\\": "packages/Webkul/Admin/src",
            "Webkul\\Ui\\": "packages/Webkul/Ui/src",
            "Webkul\\Category\\": "packages/Webkul/Category/src",
            "Webkul\\Checkout\\": "packages/Webkul/Checkout/src",
            "Webkul\\Attribute\\": "packages/Webkul/Attribute/src",
            "Webkul\\Shop\\": "packages/Webkul/Shop/src",
            "Webkul\\Core\\": "packages/Webkul/Core/src",
            "Webkul\\Customer\\": "packages/Webkul/Customer/src",
            "Webkul\\Inventory\\": "packages/Webkul/Inventory/src",
            "Webkul\\Product\\": "packages/Webkul/Product/src",
            "Webkul\\Theme\\": "packages/Webkul/Theme/src",
            "Webkul\\Shipping\\": "packages/Webkul/Shipping/src",
            "Webkul\\Payment\\": "packages/Webkul/Payment/src",
            "Webkul\\Paypal\\": "packages/Webkul/Paypal/src",
            "Webkul\\Sales\\": "packages/Webkul/Sales/src",
            "Webkul\\Tax\\": "packages/Webkul/Tax/src",
            "Webkul\\API\\": "packages/Webkul/API",
            "Webkul\\CatalogRule\\": "packages/Webkul/CatalogRule/src",
            "Webkul\\CartRule\\": "packages/Webkul/CartRule/src",
            "Webkul\\Rule\\": "packages/Webkul/Rule/src",
            "Webkul\\CMS\\": "packages/Webkul/CMS/src",
            "Webkul\\Velocity\\": "packages/Webkul/Velocity/src",
            "Webkul\\BookingProduct\\": "packages/Webkul/BookingProduct/src",
            "Webkul\\SAASCustomizer\\": "packages/Webkul/SAASCustomizer/src",
            "Mega\\HeaderFooter\\": "packages/Mega/HeaderFooter/src",
            "Mega\\Phonelogin\\": "packages/Mega/Phonelogin/src",
            "Mega\\SmsNotifications\\": "packages/Mega/SmsNotifications/src",
            "Webkul\\PWA\\": "packages/Webkul/PWA/src",
            "Devvly\\Subscription\\": "packages/Devvly/Subscription/src",
            "Webkul\\Stripe\\": "packages/Webkul/Stripe/src",
            "Webkul\\Authorize\\": "packages/Webkul/Authorize/src",
            "Webkul\\Braintree\\": "packages/Webkul/Braintree/src",
            "Devvly\\OnBoarding\\": "packages/Devvly/OnBoarding/src",
            "Devvly\\Ffl\\": "packages/Devvly/Ffl/src",
            "Webkul\\Bulkupload\\": "packages/Webkul/Bulkupload/src",
            "Webkul\\RMA\\": "packages/Webkul/RMA/src",
            "Webkul\\TableRate\\": "packages/Webkul/TableRate/src",
            "Devvly\\FflOnBoarding\\": "packages/Devvly/FflOnBoarding/src",
            "Webkul\\Marketplace\\": "packages/Webkul/Marketplace/src",
            "Webkul\\MarketplaceSaaS\\": "packages/Webkul/MarketplaceSaaS/src",
            "Devvly\\CustomBlocks\\": "packages/Devvly/CustomBlocks/src",
            "Devvly\\ClearentPayment\\": "packages/Devvly/ClearentPayment/src",
            "Devvly\\FluidPayment\\": "packages/Devvly/FluidPayment/src",
            "Devvly\\DistributorImport\\":"packages/Devvly/DistributorImport/src",
            "Webkul\\MarketplaceUspsShipping\\": "packages/Webkul/MarketplaceUspsShipping/src",
            "Webkul\\MarketplaceFedExShipping\\": "packages/Webkul/MarketplaceFedExShipping/src",
            "Webkul\\MarketplaceUpsShipping\\": "packages/Webkul/MarketplaceUpsShipping/src",
            "Devvly\\ElasticSearch\\":"packages/Devvly/ElasticSearch/src",
            "Webkul\\Tripple\\": "packages/Webkul/Tripple/src",
            "Devvly\\Blog\\":"packages/Devvly/Blog/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "laravel/dusk"
            ]
        },
        "patches": {
        },
        "merge-plugin": {
            "include": [
                "resources/pagebuilder-themes/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "Webkul\\Core\\Events\\ComposerEvents::postCreateProject"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "test": [
            "set -e",
            "@php artisan migrate:fresh --env=testing",
            "vendor/bin/codecept run unit",
            "vendor/bin/codecept run functional",
            "vendor/bin/codecept run trigger"
        ]
    },
    "config": {
        "preferred-install": "source",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
